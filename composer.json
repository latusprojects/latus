{
  "name": "latusprojects/latus",
  "type": "library",
  "description": "",
  "authors": [
    {
      "name": "Max Latussek",
      "email": "latusschnecke@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "8.*",
    "latusprojects/latus-plugins": "0.2.*",
    "latusprojects/latus-model-repositories": "0.1.*",
    "latusprojects/latus-permissions": "0.2.*",
    "latusprojects/latus-installer": "0.1.*",
    "latusprojects/latus-ui": "0.1.*",
    "latusprojects/latus-content": "0.1.*",
    "latusprojects/latus-composer-plugins": "0.2.*",
    "latusprojects/latus-laravel-application": "0.1.*",
    "latusprojects/latus-permalink": "0.1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0",
    "orchestra/testbench": "v6.19.0",
    "nunomaduro/larastan": "^0.7.12"
  },
  "autoload": {
    "psr-4": {
      "Latus\\Latus\\": "src/",
      "Latus\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Latus\\Latus\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Latus\\Latus\\Providers\\RouteServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ]
  },
  "repositories": {
    "0": {
      "type": "composer",
      "url": "https://latusprojects.repo.repman.io"
    }
  },
  "minimum-stability": "dev"
}