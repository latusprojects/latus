{
  "name": "latusprojects/latus",
  "type": "library",
  "description": "",
  "authors": [
    {
      "name": "Max Latussek",
      "email": "latusschnecke@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "8.*",
    "latusprojects/latus-plugins": "dev-develop",
    "latusprojects/latus-model-repositories": "dev-develop",
    "latusprojects/latus-permissions": "dev-develop",
    "latusprojects/latus-installer": "dev-develop"
  },
  "require-dev": {
    "latusprojects/latus-phpunit-commons": "dev-master",
    "phpunit/phpunit": "^9.5.0",
    "orchestra/testbench": "v6.19.0"
  },
  "autoload": {
    "psr-4": {
      "Latus\\Latus\\": "src/",
      "Latus\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Latus\\Latus\\Tests\\": "tests/"
    }
  },
  "repositories": {
    "0": {
      "type": "path",
      "url": "../latus-plugins"
    },
    "1": {
      "type": "path",
      "url": "../latus-model-repositories"
    },
    "2": {
      "type": "path",
      "url": "../latus-permissions"
    },
    "3": {
      "type": "path",
      "url": "../latus-installer"
    },
    "4": {
      "type": "path",
      "url": "../latus-phpunit-commons"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Latus\\Latus\\LatusServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ]
  }
}